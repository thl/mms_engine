<% if !@medium.nil? %>
     $('#primary').html("<%= escape_javascript(render(:partial => 'media/show')) %>")
<% end %>
$('#secondary').html("<%= escape_javascript(render(:partial => 'media_index')) %>")
$('.selected_branch').removeClass('selected_branch')
$("#<%= @topic.id %>_div").html("<%= escape_javascript(render(:partial => 'expanded', :object => @topic, :locals => {:margin_depth => @margin_depth})) %>")