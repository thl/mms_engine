<h1><%= ts 'list.ing', :what => Video.model_name.human(:count => :many).titleize %></h1>
<table border="1" class="normal">
  <tr>
    <th><%= ts 'id.record', :what => Video.model_name.human.titleize %></th>
    <th><%= Video.human_attribute_name('photographer').s %></th>
    <th><%= QualityType.model_name.human.titleize.s %></th>
  </tr>
<% for video in @videos %>
  <tr>
    <td><%= link_to video.id, video_path(video) %></td>
    <td><%=h video.photographer.fullname if !video.photographer.nil? %></td>
    <td><%=h(video.quality_type.title).s %></td>
  </tr>
<% end %>
</table>
<p>
<%= link_to ts('new.record', :what => Video.model_name.human), new_video_path %>
</p>